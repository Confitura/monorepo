{
  "name": "confitura-page",
  "version": "61.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "mock": "node mock/mock.js",
    "start-with-proxy": "ng serve --aot --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:full": "yarn lint && yarn test && yarn build --prod",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod  && yarn deploy:only",
    "deploy:only": "cd deployment && ansible-playbook confitura-page.yml",
    "release:patch": "yarn build:full && npm version patch && git push --follow-tags",
    "release:minor": "yarn build:full && npm version minor && git push --follow-tags",
    "release:major": "yarn build:full && npm version major && git push --follow-tags",
    "stats": "yarn stats:build --prod && yarn stats:serve",
    "stats:build": "yarn build --stats-json",
    "stats:serve": "webpack-bundle-analyzer dist/stats.json"
  },
  "license": "ISC",
