{
  "name": "confitura-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "yarn deploy:public --extra-vars 'edition=next'",
    "build:full": "yarn lint && yarn build:prod",
    "build:prod": "yarn build --prod --modern",
