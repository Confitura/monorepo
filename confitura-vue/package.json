    "dayjs": "^1.7.8",
    "gsap": "^2.0.2",
    "intersection-observer": "^0.5.1",
    "jssha": "^2.3.1",
    "linkifyjs": "^2.1.8",
    "marked": "^0.6.2",
    "materialize-css": "^1.0.0-rc.2",
    "normalize.css": "^8.0.1",
    "v-hotkey": "^0.4.0",
    "vee-validate": "^2.2.10",
    "vue": "^3.2.32",
    "vue-class-component": "^7.2.6",
    "vue-material": "^1.0.0-beta-11",
    "vue-property-decorator": "^9.1.2",
