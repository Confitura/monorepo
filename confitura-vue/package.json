{
  "name": "confitura-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:prod": "yarn build --prod --modern",
    "build:full": "yarn lint && yarn build:prod",
    "lint": "vue-cli-service lint",
    "deploy": "yarn deploy:public --extra-vars 'edition=next'",
    "deploy:public": "yarn build:full && yarn deploy:only",
    "deploy:only": "ansible-playbook deployment/confitura-page.yml",
    "sb": "start-storybook -p 9001 -c .storybook",
    "mock": "http-server ./mock"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.7.8",
    "gsap": "^2.0.2",
    "intersection-observer": "^0.5.1",
    "linkifyjs": "^2.1.8",
    "materialize-css": "^1.0.0-rc.2",
    "normalize.css": "^8.0.1",
    "showdown": "^1.9.0",
    "vue": "^2.6.8",
    "vue-class-component": "^7.0.1",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.13.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-backgrounds": "^5.0.1",
    "@storybook/vue": "^5.0.0",
    "@types/gsap": "^1.20.1",
    "@types/materialize-css": "^1.0.6",
    "@types/showdown": "^1.9.2",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.0.1",
    "typescript": "^3.0.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.8"
  }
}
