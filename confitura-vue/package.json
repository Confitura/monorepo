    "eslint-plugin-vue": "^8.0.3",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "prettier": "^2.4.1",
    "sass-loader": "^7.0.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.5.5",
    "vue-loader": "^17.0.0"
